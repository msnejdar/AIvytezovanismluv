{
  "name": "ai-intelligence-search",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:comprehensive": "node src/test/testRunner.js",
    "test:normalization": "vitest run src/test/documentNormalizer.test.js",
    "test:fuzzy": "vitest run src/test/fuzzyMatching.test.js",
    "test:highlighting": "vitest run src/test/highlighting.test.js",
    "test:performance": "vitest run src/test/performance.test.js",
    "test:edge": "vitest run src/test/edgeCases.test.js",
    "test:api": "vitest run src/test/apiIntegration.test.js",
    "test:czech": "vitest run src/test/documentNormalizer.test.js src/test/fuzzyMatching.test.js --reporter=verbose"
  },
  "dependencies": {
    "@types/node": "^24.5.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "framer-motion": "^12.23.22",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "happy-dom": "^12.10.3",
    "jsdom": "^23.2.0",
    "supertest": "^6.3.4",
    "vite": "^7.1.7",
    "vitest": "^1.6.0"
  }
}
